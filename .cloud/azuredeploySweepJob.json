{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspaceName": {
      "type": "string",
      "metadata": {
        "description": "Specifies the name of the Azure Machine Learning Workspace which will run this job."
      }
    },
    "jobName": {
      "type": "string",
      "metadata": {
        "description": "The name of the job."
      }
    },
    "experimentName": {
      "defaultValue": "Default",
      "type": "string",
      "metadata": {
        "description": "The name of the experiment the job belongs to. If not set, the job is placed in the \"Default\" experiment."
      }
    }
  },
  "resources": [
    {
      "name": "[concat(parameters('workspaceName'), '/', parameters('jobName'))]",
      "type": "Microsoft.MachineLearningServices/workspaces/jobs",
      "apiVersion": "2021-03-01-preview",
      "dependsOn": [
        "[parameters('workspaceName')]",
        "[variables('codeVersionResourceId')]",
        "[variables('dataVersionResourceId')]",
        "[variables('computeResourceId')]"
      ],
      "properties": {
        "description": "string",
        "properties": {},
        "jobType": "Sweep",
        "compute": {
          "instanceCount": "int",
          "instanceType": "string",
          "isLocal": "bool",
          "location": "string",
          "properties": {},
          "target": "string"
        },
        "priority": "int",
        "timeout": "string",
        "algorithm": "string",
        "earlyTermination": {},
        "experimentName": "string",
        "maxConcurrentTrials": "int",
        "maxTotalTrials": "int",
        "objective": {
          "goal": "string",
          "primaryMetric": "string"
        },
        "searchSpace": {},
        "trial": {
          "codeId": "string",
          "command": "string",
          "distribution": {},
          "environmentId": "string",
          "environmentVariables": {},
          "inputDataBindings": {},
          "outputDataBindings": {},
          "timeout": "string"
        }
      }
    }
  ],
  "outputs": {
    "result": {
      "type": "Object",
      "value": "[reference(resourceId('Microsoft.MachineLearningServices/workspaces/jobs', parameters('workspaceName'), parameters('jobName')))]"
    }
  }
}
