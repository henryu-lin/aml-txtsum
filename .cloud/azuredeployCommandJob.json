{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspaceName": {
      "type": "string",
      "metadata": {
        "description": "Specifies the name of the Azure Machine Learning Workspace which will run this job."
      }
    },
    "jobName": {
      "type": "string",
      "metadata": {
        "description": "The name of the job."
      }
    },
    "experimentName": {
      "defaultValue": "Default",
      "type": "string",
      "metadata": {
        "description": "The name of the experiment the job belongs to. If not set, the job is placed in the \"Default\" experiment."
      }
    }
  },
  "resources": [
    {
      "name": "[concat(parameters('workspaceName'), '/', parameters('jobName'))]",
      "type": "Microsoft.MachineLearningServices/workspaces/jobs",
      "apiVersion": "2021-03-01-preview",
      "dependsOn": [
        "[parameters('workspaceName')]",
        "[variables('codeVersionResourceId')]",
        "[variables('dataVersionResourceId')]",
        "[variables('computeResourceId')]"
      ],
      "properties": {
        "properties": {},
        "jobType": "Command",
        "codeId": "string",
        "command": "string",
        "compute": {
          "instanceCount": "int",
          "instanceType": "string",
          "isLocal": "bool",
          "location": "string",
          "properties": {},
          "target": "string"
        },
        "distribution": {
          "distributionType": "PyTorch",
          "processCount": "int"
        },
        "environmentId": "string",
        "environmentVariables": {},
        "experimentName": "string",
        "inputDataBindings": {},
        "outputDataBindings": {}
      }
    }
  ]
}
